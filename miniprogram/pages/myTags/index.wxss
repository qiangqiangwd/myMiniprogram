/* miniprogram/pages/myTags/index.wxss */
.container{
    padding-bottom: 140rpx;
}
.newBtn {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
}
.newBtn .btn {
  width: 120rpx;
  height: 120rpx;
  background: linear-gradient(
    180deg,
    #F56C6C 0%,
    #e6a23c 100%
  );
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  margin: 0 auto 20rpx;
  position: relative;
  box-shadow: 0 0 15rpx #fff;
}

.newBtn .btn::after,
.newBtn .btn::before {
  content: "";
  position: absolute;
  top: 50%;
  left: 50%;
  background: #ffffff;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  -o-transform: translate(-50%, -50%);
}
.newBtn .btn::after {
  width: 50rpx;
  height: 5rpx;
}
.newBtn .btn::before {
  height: 50rpx;
  width: 5rpx;
}
